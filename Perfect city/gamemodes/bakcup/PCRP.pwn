			return 1;
		}

		format(string, sizeof(string), "[AdmCmdExe]: %s ha enviado a %s (ID: %i) el (RID: %i) a los ayudantes.", GetPlayerNameEx(playerid), GetPlayerNameEx(Reports[reportid][ReportFrom]),Reports[reportid][ReportFrom],reportid);
