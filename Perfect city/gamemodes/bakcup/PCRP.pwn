		{
		    new Float:oldx, Float:oldy, Float:oldz;
			oldx = FVINFO[f][fv][fvSpawnxtmp];
			oldy = FVINFO[f][fv][fvSpawnytmp];
			oldz = FVINFO[f][fv][fvSpawnztmp];
