				}
			}
		}
		else return SendClientMessageEx(playerid, COLOR_WHITE, "No puedes pagar estas drogas.");
	}
	else SendClientMessageEx(playerid, COLOR_WHITE, "No perteneces a una banda/familia.");
	return 1;
}
