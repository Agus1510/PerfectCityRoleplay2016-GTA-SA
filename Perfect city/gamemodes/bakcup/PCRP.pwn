		}
		if(phonenumb == Info[playerid][pPnumber]) return SendClientMessageEx(playerid, COLOR_WHITE, " Está ocupado.");
		foreach(Player, i){
			if(Info[i][pPnumber] == phonenumb && phonenumb != 0){
