					format(szSpeed, sizeof(szSpeed),"~n~~n~~n~~n~~n~~n~~n~~n~~n~~g~%.0f KPH ~w~- Gas: %d", fCurrentSpeed, VehicleFuel[iVehicle]);
				}
			}
