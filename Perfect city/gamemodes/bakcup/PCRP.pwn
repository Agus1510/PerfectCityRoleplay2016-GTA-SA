timer SyncUp[60000]()
{
    new hour,minuite,second;
    if(PayDayLeft >= 1)
	{
	    PayDayLeft--;
	}
	else
	{
        PayDayLeft = 60;
	}
	SyncTime();
	SyncMinTime();
	DollahScoreUpdate();
	gettime(hour,minuite,second);
	FixHour(hour);
	hour = shifthour;
	if(PayDayLeft == 0) PayDay();
	foreach(Player, i)
	{
	   	if(Info[i][pBeingSentenced] > 1) Info[i][pBeingSentenced]--;
