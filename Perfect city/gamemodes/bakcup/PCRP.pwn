			FVINFO[f][fv][fvSpawnxtmp] = newx;
			FVINFO[f][fv][fvSpawnytmp] = newy;
			FVINFO[f][fv][fvSpawnztmp] = newz;
			FVINFO[f][fv][fvSpawnatmp] = newangle;
