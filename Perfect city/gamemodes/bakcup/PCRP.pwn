	SetPVarInt(playerid, "ArenaMode", 0);
	SetPVarInt(playerid, "IsInArena", 0);
	SetPlayerPos(playerid, GetPVarFloat(playerid, "pbOldX"), GetPVarFloat(playerid, "pbOldY"), GetPVarFloat(playerid, "pbOldZ"));
